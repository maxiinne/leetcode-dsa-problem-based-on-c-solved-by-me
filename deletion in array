#include<stdio.h>

int delete(struct array*arr , int index)
{
    int x = 0;
    if(index >=0 && index<arr->length)
    {
        x = arr->A[index]
        for(i = index; i<arr->length-1; i++)
        {
            arr->A[i] = arr->A[i+1];
        }
        arr->length--;
        return x;
    }
    int main()
    {
        struct array arr = {{2,3,4,5,6},10,5};
        printf("delete(&arr,4)");
        display(arr);
        
    }
    return 0;
}
